# RDL file to process
RDL_FILE=dt5771_regmap.rdl

# Prefix for register defines (C headers only)
PREFIX="DT5771_"

# grogu runs
../../../ni-cores/tools/grogu/grogu.py \
    -r ${RDL_FILE} \
    -t ../../../ni-cores/tools/grogu/example_files \
    -m MY_MODULE_TEMPLATE.sv \
    -p my_package_template.sv \
    -x ${PREFIX}

# Show output products
cat grogu.gen/DT5771_REGMAP/csr.tree
